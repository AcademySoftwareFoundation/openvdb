
bool@testbool = false % true;
int16@testint16 = 3 % 2;
int32@testint32 = 3 % 2;
int64@testint64 = 3l % 2l;
double@testdouble = 2.3 % 1.1;
float@testfloat = 2.3f % 1.1f;
vec2d@testvec2d = {4.1, 5.3} % {1.1, 2.3};
vec2f@testvec2f = {4.1f, 5.3f} % {1.1f, 2.3f};
vec2i@testvec2i = {3, 4} % {1, 2};
vec3d@testvec3d = {4.1,  5.3, 6.3} % {1.1,  2.3 , 4.3};
vec3f@testvec3f = {4.1f, 5.3f, 6.3f} % {1.1f, 2.3f, 4.3f};
vec3i@testvec3i = {4, 5, 6} % {1, 2, 3};
vec4d@testvec4d = {5.1, 6.3, 7.3, 8.4} % {1.1,  2.3, 4.3, 5.4};
vec4f@testvec4f = {5.1f, 6.3f, 7.3f, 8.4f} % {1.1f, 2.3f, 4.3f, 5.4f};
vec4i@testvec4i = {5, 6, 7, 8} % {1, 2, 3, 4};
